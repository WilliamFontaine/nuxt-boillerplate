{
  "name": "nuxt-boilerplate",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "author": "William Fontaine",
  "license": "MIT",
  "homepage": "https://github.com/WilliamFontaine/nuxt-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/WilliamFontaine/nuxt-boilerplate/issues"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "nuxt preview",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint . --fix --ignore-pattern volumes/",
    "lint:prettier": "prettier --write --list-different .",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "test": "pnpm run test:unit && pnpm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "changelog:generate": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "changelog:preview": "conventional-changelog -p conventionalcommits -u",
    "version:check": "echo \"Current version: $(node -p \"require('./package.json').version\")\" && git status --porcelain",
    "tag:patch": "pnpm run changelog:generate && git add CHANGELOG.md && git commit -m \"chore(release): update changelog\" && pnpm version patch && git push --follow-tags",
    "tag:minor": "pnpm run changelog:generate && git add CHANGELOG.md && git commit -m \"chore(release): update changelog\" && pnpm version minor && git push --follow-tags",
    "tag:major": "pnpm run changelog:generate && git add CHANGELOG.md && git commit -m \"chore(release): update changelog\" && pnpm version major && git push --follow-tags"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.57",
    "@nuxt/image": "1.10.0",
    "@nuxt/ui": "3.2.0",
    "@nuxtjs/i18n": "9.5.6",
    "@prisma/nuxt": "^0.3.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nuxt/eslint": "1.5.2",
    "@nuxt/test-utils": "3.19.2",
    "@playwright/test": "1.54.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "conventional-changelog-cli": "5.0.0",
    "conventional-changelog-writer": "8.2.0",
    "eslint": "^9.31.0",
    "happy-dom": "18.0.1",
    "husky": "^9.1.7",
    "nuxt": "4.0.0",
    "playwright-core": "1.54.1",
    "prettier": "^3.6.2",
    "prisma": "^6.12.0",
    "typescript": "^5.8.3",
    "vitest": "3.2.4"
  }
}
